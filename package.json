{
  "name": "sketch-unsplasher",
  "description": "Fills shapes or symbols with images from Unsplash",
  "author": "Giles Perry <giles@gilesperry.info> (http://gilesperry.info/)",
  "version": "1.2.0",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "Unsplasher",
    "manifest": "src/manifest.json",
    "identifier": "com.gilesperry.sketch-unsplasher",
    "main": "SketchUnsplasher.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/perrysmotors/sketch-unsplasher.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@skpm/builder": "^0.5.2"
  }
}
